{
  "tpl if": {
    "prefix": "if",
    "body": [
      "{{#if ${1:condition}}}",
      "\t$2",
      "{{/if}}"
    ]
  },
  "tpl ifelse": {
    "prefix": "ifelse",
    "body": [
      "{{#if ${1:condition}}}",
      "\t$2",
      "{{else}}",
      "\t$3",
      "{{/if}}"
    ]
  },
  "tpl ifelseif": {
    "prefix": "ifelseif",
    "body": [
      "{{#if ${1:condition}}}",
      "\t$2",
      "{{else if ${3:condition}}}",
      "\t$4",
      "{{/if}}"
    ]
  },
  "tpl ifelseifelse": {
    "prefix": "ifelseifelse",
    "body": [
      "{{#if ${1:condition}}}",
      "\t$2",
      "{{else if ${3:condition}}}",
      "\t$4",
      "{{else}}",
      "\t$5",
      "{{/if}}"
    ]
  },
  "tpl unless": {
    "prefix": "unless",
    "body": [
      "{{#unless ${1:condition}}}",
      "\t$2",
      "{{/unless}}"
    ]
  },
  "tpl each": {
    "prefix": "each",
    "body": [
      "{{#each ${1:object}}}",
      "\t$2",
      "{{/each}}"
    ]
  },
  "tpl with": {
    "prefix": "with",
    "body": [
      "{{#with ${1:object}}}",
      "\t$2",
      "{{/with}}"
    ]
  },
  "tpl ifEquals": {
    "prefix": "ifequals",
    "body": [
      "{{#ifEquals ${1:object} ${2:value}}}",
      "\t$3",
      "{{/ifEquals}}"
    ]
  },
  "tpl unlessEquals": {
    "prefix": "unlessequals",
    "body": [
      "{{#unlessEquals ${1:object} ${2:value}}}",
      "\t$3",
      "{{/unlessEquals}}"
    ]
  }
}
